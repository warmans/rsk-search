{
  "name": "gui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration development",
    "start-prod": "ng serve --proxy-config=src/proxy.conf.prod.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "generate-api-client": "rm -rf ./src/app/lib/api-client && api-client-generator -s ./src/assets/swagger.json -o ./src/app/lib/api-client -t all",
    "docker-build": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-build.sh",
    "docker-run": "docker run -it -p 8080:80 warmans/rsk-search-gui:latest",
    "docker-publish": "DOCKER_IMAGE_NAME=rsk-search-gui ../deploy/script/docker-publish.sh",
    "release": "npm run build && npm run docker-build && npm run docker-publish",
    "check-deps": "npx npm-check-updates",
    "update-deps": "npx npm-check-updates -i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.13",
    "@angular/common": "18.2.13",
    "@angular/compiler": "18.2.13",
    "@angular/core": "18.2.13",
    "@angular/forms": "18.2.13",
    "@angular/localize": "18.2.13",
    "@angular/platform-browser": "18.2.13",
    "@angular/platform-browser-dynamic": "18.2.13",
    "@angular/router": "18.2.13",
    "@ng-bootstrap/ng-bootstrap": "^17.0.0",
    "@popperjs/core": "^2.11.8",
    "@sentry/angular": "^9.40.0",
    "@types/w3c-web-hid": "^1.0.3",
    "@types/w3c-web-usb": "^1.0.6",
    "angular-star-rating": "^7.0.0",
    "animate.css": "^4.1.1",
    "api-client-generator": "^4.7.1",
    "bootstrap": "^4.6.0",
    "bootstrap-icons": "^1.11.3",
    "chart.js": "^3.9.1",
    "css-star-rating": "^1.3.1",
    "date-fns": "^2.29.3",
    "diff2html": "^3.4.48",
    "marked": "^12.0.1",
    "ng2-charts": "^3.0.1",
    "rxjs": "~6.5.4",
    "tslib": "^2.4.0",
    "wavesurfer.js": "^7.7.13",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.20",
    "@angular/cli": "18.2.20",
    "@angular/compiler-cli": "18.2.13",
    "@types/jasmine": "3.8.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^18.0.0",
    "codelyzer": "^6.0.2",
    "jasmine-core": "3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~5.5.4"
  }
}
